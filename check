#!/bin/bash
start_time=`date +%s.%N`
./compare/compare -opt 10000000 &> /dev/null
res=$?
end_time=`date +%s.%N`
elapsed_time=`echo "$end_time - $start_time" | bc `
if [ $res -ne 0 ]; then
  elapsed_time="None"
fi
echo 'compare_elapsed:' $elapsed_time

start_time=`date +%s.%N`
./empty/empty -opt 10000000 &> /dev/null
res=$?
end_time=`date +%s.%N`
elapsed_time=`echo "$end_time - $start_time" | bc `
if [ $res -ne 0 ]; then
  elapsed_time="None"
fi
echo 'empty_elapsed:' $elapsed_time

start_time=`date +%s.%N`
./equality/equality -opt 1000 &> /dev/null
res=$?
end_time=`date +%s.%N`
elapsed_time=`echo "$end_time - $start_time" | bc `
if [ $res -ne 0 ]; then
  elapsed_time="None"
fi
echo 'equality_elapsed:' $elapsed_time

./file-io/gen 100000000
start_time=`date +%s.%N`
./file-io/read -opt large.txt &> /dev/null
res=$?
end_time=`date +%s.%N`
elapsed_time=`echo "$end_time - $start_time" | bc `
if [ $res -ne 0 ]; then
  elapsed_time="None"
fi
echo 'file-io_elapsed:' $elapsed_time

start_time=`date +%s.%N`
./list/list -opt 10000000 &> /dev/null
res=$?
end_time=`date +%s.%N`
elapsed_time=`echo "$end_time - $start_time" | bc `
if [ $res -ne 0 ]; then
  elapsed_time="None"
fi
echo 'list_elapsed:' $elapsed_time

start_time=`date +%s.%N`
./regexp/regexp -opt 1000000 &> /dev/null
res=$?
end_time=`date +%s.%N`
elapsed_time=`echo "$end_time - $start_time" | bc `
if [ $res -ne 0 ]; then
  elapsed_time="None"
fi
echo 'regexp_elapsed:' $elapsed_time

./serialize/gen 1000000 &> /dev/null
start_time=`date +%s.%N`
./serialize/gen -opt 1000000 &> /dev/null
res=$?
end_time=`date +%s.%N`
elapsed_time=`echo "$end_time - $start_time" | bc `
if [ $res -ne 0 ]; then
  elapsed_time="None"
fi
echo 'serialize_gen_elapsed:' $elapsed_time

start_time=`date +%s.%N`
./serialize/read -opt large.out &> /dev/null
res=$?
end_time=`date +%s.%N`
elapsed_time=`echo "$end_time - $start_time" | bc `
if [ $res -ne 0 ]; then
  elapsed_time="None"
fi
echo 'serialize_read_elapsed:' $elapsed_time

start_time=`date +%s.%N`
./string/string -opt 1000000 &> /dev/null
res=$?
end_time=`date +%s.%N`
elapsed_time=`echo "$end_time - $start_time" | bc `
if [ $res -ne 0 ]; then
  elapsed_time="None"
fi
echo 'string_elapsed:' $elapsed_time

start_time=`date +%s.%N`
./tail-recursion/tail -opt 10000000 &> /dev/null
res=$?
end_time=`date +%s.%N`
elapsed_time=`echo "$end_time - $start_time" | bc `
if [ $res -ne 0 ]; then
  elapsed_time="None"
fi
echo 'tail-recursion_elapsed:' $elapsed_time
